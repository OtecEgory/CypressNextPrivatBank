{
  "name": "cypressproject",
  "version": "1.0.0",
  "description": "Cy project test bank",
  "main": "index.js",
  "scripts": {
    "test": "node cypress/scripts/runner.js",
    "allure:report": "allure generate allure-results --clean -o allure-report",
    "allure:clear": "rm -r allure-results/ allure-report cypress/screenshots || true",
    "allure:history": "mv -f allure-report/history allure-results/history && rm -r allure-report || true",
    "runHeadless": "./node_modules/.bin/cypress run --headless",
    "runHeaded": "./node_modules/.bin/cypress run --headed --browser chrome "
  },
  "author": "Egor Antsyshkin",
  "license": "ISC",
  "dependencies": {
    "cypress": "^7.1.0"
  },
  "devDependencies": {
    "@shelex/cypress-allure-plugin": "^2.5.1",
    "allure-commandline": "^2.13.8"
  }
}
